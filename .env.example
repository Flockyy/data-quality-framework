# Environment variables for Data Quality Framework

# Database
DATABASE_URL=postgresql://dqf_user:dqf_password@localhost:5432/data_quality

# Redis
REDIS_URL=redis://localhost:6379

# Email notifications
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
EMAIL_FROM=data-quality@company.com

# Slack notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Webhook notifications
API_TOKEN=your-api-token

# PagerDuty (optional)
PAGERDUTY_KEY=your-pagerduty-integration-key

# Twilio (optional)
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token

# AWS (optional)
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/dqf.log

# Application
DQF_ENV=development  # development, staging, production
DQF_DEBUG=true
